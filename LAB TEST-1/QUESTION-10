def getPermutation(n, k):
    import math
    numbers = list(range(1, n + 1))
    permutation = []
    k -= 1   
    while n > 0:
        n -= 1
        index, k = divmod(k, math.factorial(n))
        permutation.append(numbers.pop(index)) 
    return ''.join(map(str, permutation))
n = 3
k = 3
print(getPermutation(n, k)) 
