def int_to_roman(num):
    val = [
        1000, 900, 500, 400,
        100, 90, 50, 40,
        10, 9, 5, 4,
        1
    ]
    syms = [
        "M", "CM", "D", "CD",
        "C", "XC", "L", "XL",
        "X", "IX", "V", "IV",
        "I"
    ]
    roman_num = ''
    for i in range(len(val)):
        while num >= val[i]:
            num -= val[i]
            roman_num += syms[i]
    return roman_num
num1 = 1994
print(int_to_roman(num1))  # Output: "MCMXCIV"
